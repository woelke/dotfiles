{
  "deps": [
    {
      "yay": [
        "zsh",
        "vivid",
        "lsd",
        "bat",
        "ripgrep",
        "fd",
        "fzf"
      ]
    }
  ],
  "links": [
    {
      "dest": "~/.zshrc",
      "name": ".zshrc"
    }
  ],
  "scripts": [
    "sh -c \"$(curl -fsSL https://raw.githubusercontent.com/woelke/ohmyzsh/master/tools/install.sh)\"",
    "sudo chsh -s $(which zsh) $(whoami)"
  ]
}
